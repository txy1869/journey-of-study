#include <stdio.h>
#include <math.h>
int main()
{
	int i = 0, a = 0, b = 0,  c = 0,j = 0;
	for (i = 0; i < 999; i++)
	{
		a = i / 100;
		b = (i / 10) % 10;
		c = (i % 100) % 10;
		a = pow(a, 3);
		b = pow(b, 3);
		c = pow(c, 3);
		j = a + b + c;
		if (j == i)
		printf("%d ", i);

	}
	system("pause");
	return 0;
}

